PRODDIR/var/*log {
    missingok
    notifempty
}

PRODDIR/var/xidebug.log {
    missingok
    notifempty
    size 100M
    create 0660 APACHEUSER NAGIOSGROUP
    rotate 1
    compress
}

PRODDIR/var/xidebug.log.backtrace {
    missingok
    notifempty
    size 100M
    create 0660 APACHEUSER NAGIOSGROUP
    rotate 1
    compress
}